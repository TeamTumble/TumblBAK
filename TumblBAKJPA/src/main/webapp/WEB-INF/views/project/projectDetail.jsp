			$("#f_data").attr({
				"method" : "get",
				"action" : "/support/supportCreate.do"
			});
			$("#f_data").submit();

		});

		/* 수정하기 버튼 클릭 시 처리 이벤트 */
		$("#updateBtn").click(function() {
			var pno = $(this).parents("tr").attr("data-num");
			$("#pno").val(pno);
			console.log("글번호 : " + pno);
			//상세 페이지로 이동하기 위해 form추가 (id : detailForm)
			$("#f_data").attr({
